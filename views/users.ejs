<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <meta name="viewport" content="width=device-width, initial-scale=1.0">
  <title>Document</title>
  <link rel="stylesheet" href="/styles.css">
</head>
<body>
  <div><h1>User's List for the exercises</h1></div>
  <div class="container">
    
      <% users.forEach(function(user) { %>
        <div class="card">
        <p><%= user.name %> </p>
        <p>User's Id: <%= user.id %> </p>
        <p><%= user.username %> </p>
        <p><%= user.email %> </p>
      </div>
      <% }); %>
      
  </div>
 
</body>
</html>